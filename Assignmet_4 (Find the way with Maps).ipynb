    "def triple(number):\n",
    "    return number * 3\n",
    "\n",
    "numbers = [1, 2, 3, 4, 5, 6, 7]\n",
    "\n",
    "tripled_numbers = list(map(triple, numbers))\n",
    "\n",
    "print(\"Original numbers:\", numbers)\n",
    "print(\"Triple of each number:\", tripled_numbers)"
